{
  "name": "shared-types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shared-types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/shared-types",
        "main": "apps/shared-types/src/index.ts",
        "tsConfig": "apps/shared-types/tsconfig.lib.json",
        "assets": ["apps/shared-types/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/shared-types/**/*.ts",
          "apps/shared-types/package.json"
        ]
      }
    }
  },
  "tags": []
}
